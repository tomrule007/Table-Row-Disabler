{
  "name": "Table Row Locker",
  "version": "0.1.0",
  "description": "Chrome Extension that allows user to lock table rows to prevent data entry.",
  "permissions": ["storage", "activeTab", "contextMenus"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Table-Row-Locker v1.0.0\nClick to Enable!",
    "default_icon": {
      "16": "img/tableRowLockerDisabledIcon16.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["tableRowLocker.js"],
      "css": ["tableRowLocker.css"]
    }
  ],
  "icons": {
    "16": "img/tableRowLockerIcon16.png",
    "48": "img/tableRowLockerIcon48.png",
    "128": "img/tableRowLockerIcon128.png"
  },

  "manifest_version": 2
}
