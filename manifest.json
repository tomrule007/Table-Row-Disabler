{
  "name": "Table Row Locker",
  "version": "1.0.0",
  "description": "Chrome Extension that allows user to lock table rows to prevent data entry.",
  "web_accessible_resources": ["tableRowLocker.js"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "highlightActiveRowIcon16.png",
    "48": "highlightActiveRowIcon48.png",
    "128": "highlightActiveRowIcon128.png"
  },

  "manifest_version": 2
}
